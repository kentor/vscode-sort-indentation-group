{
  "name": "sort-indentation-group",
  "displayName": "sort-indentation-group",
  "description": "Sort surrounding lines with the same indentation as the line under the cursor",
  "version": "0.0.2",
  "publisher": "kentor",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sort"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sort",
        "title": "Sort Indentation Group"
      }
    ],
    "keybindings": [
      {
        "command": "extension.sort",
        "key": "f4",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "keywords": [
    "formatter",
    "formatting",
    "indentation",
    "sort",
    "sorting",
    "vscode"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kentor/vscode-sort-indentation-group"
  },
  "devDependencies": {
    "eslint": "^3.9.0",
    "eslint-config-kentor": "^3.7.1",
    "vscode": "^1.0.0"
  }
}
